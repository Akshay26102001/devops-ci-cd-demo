{
  "family": "my-task",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["EC2", "FARGATE"],
  "containerDefinitions": [
    {
      "name": "myapp",
      "image": "IMAGE_URI",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
